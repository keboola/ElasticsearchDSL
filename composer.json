{
    "name": "keboola/elasticsearch-dsl",
    "description": "Elasticsearch DSL library",
    "type": "library",
    "homepage": "http://ongr.io",
    "license": "MIT",
    "authors": [
        {
            "name": "ONGR team",
            "homepage": "http://www.ongr.io"
        }
    ],
    "repositories":[
        {
            "type": "vcs",
            "url": "https://github.com/keboola/elasticsearch-php.git"
        }
    ],
    "require": {
        "php": ">=5.6",
        "symfony/serializer": "~2.7|~3.0|~4.0",
        "paragonie/random_compat": "*",
        "keboola/elasticsearch": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.6",
        "squizlabs/php_codesniffer": "~2.0",
        "satooshi/php-coveralls": "~1.0|~2.0"
    },
    "suggest": {
        "keboola/elasticsearch": "This library is for keboola/elasticsearch client to enhance it with DSL functionality."
    },
    "autoload": {
        "psr-4": {
            "Keboola\\ElasticsearchDSL\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Keboola\\ElasticsearchDSL\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "5.1-dev"
        }
    }
}
